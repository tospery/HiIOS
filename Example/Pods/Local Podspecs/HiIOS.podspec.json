{
  "name": "HiIOS",
  "version": "1.2.0",
  "summary": "iOS App Framework.",
  "description": "iOS App Framework using Swift.",
  "homepage": "https://github.com/tospery/HiIOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "YangJianxiang": "tospery@gmail.com"
  },
  "source": {
    "git": "https://github.com/tospery/HiIOS.git",
    "tag": "1.2.0"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "11.0"
  },
  "frameworks": [
    "UIKit",
    "CoreGraphics"
  ],
  "subspecs": [
    {
      "name": "Core",
      "source_files": "HiIOS/Core/**/*",
      "dependencies": {
        "QMUIKit/QMUICore": [
          "4.5.1"
        ],
        "FCUUID": [
          "1.3.1"
        ],
        "SwiftyBeaver": [
          "1.9.5"
        ],
        "ObjectMapper-Hi": [
          "4.2.0-v2"
        ],
        "SwifterSwift-Hi": [
          "5.3.0-v1"
        ]
      }
    },
    {
      "name": "Cache",
      "source_files": "HiIOS/Cache/**/*",
      "dependencies": {
        "HiIOS/Core": [

        ],
        "Cache": [
          "6.0.0"
        ]
      }
    },
    {
      "name": "Router",
      "source_files": "HiIOS/Router/**/*",
      "dependencies": {
        "HiIOS/Core": [

        ],
        "RxSwift": [
          "~> 6.0"
        ],
        "RxCocoa": [
          "~> 6.0"
        ],
        "URLNavigator": [
          "2.4.1"
        ]
      }
    },
    {
      "name": "Network",
      "source_files": "HiIOS/Network/**/*",
      "dependencies": {
        "HiIOS/Core": [

        ],
        "RxRelay": [
          "~> 6.0"
        ],
        "Moya/RxSwift": [
          "15.0.0"
        ]
      }
    },
    {
      "name": "Theme",
      "source_files": "HiIOS/Theme/**/*",
      "dependencies": {
        "HiIOS/Core": [

        ],
        "RxTheme": [
          "6.0.0"
        ]
      }
    },
    {
      "name": "Resources",
      "resource_bundles": {
        "Resources": [
          "HiIOS/Resources/*.*"
        ]
      },
      "pod_target_xcconfig": {
        "EXPANDED_CODE_SIGN_IDENTITY": "",
        "CODE_SIGNING_REQUIRED": "NO",
        "CODE_SIGNING_ALLOWED": "NO"
      }
    },
    {
      "name": "Components",
      "subspecs": [
        {
          "name": "JSBridge",
          "source_files": "HiIOS/Components/JSBridge/**/*",
          "frameworks": "WebKit"
        }
      ]
    },
    {
      "name": "Frame",
      "source_files": "HiIOS/Frame/**/*",
      "dependencies": {
        "HiIOS/Core": [

        ],
        "HiIOS/Cache": [

        ],
        "HiIOS/Theme": [

        ],
        "HiIOS/Router": [

        ],
        "HiIOS/Network": [

        ],
        "HiIOS/Resources": [

        ],
        "HiIOS/Components/JSBridge": [

        ],
        "RxDataSources": [
          "5.0.0"
        ],
        "NSObject+Rx": [
          "5.2.2"
        ],
        "ReactorKit": [
          "3.2.0"
        ],
        "BonMot": [
          "6.1.1"
        ],
        "Kingfisher": [
          "6.3.1"
        ],
        "DZNEmptyDataSet": [
          "1.8.1"
        ],
        "MJRefresh": [
          "3.7.5"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
